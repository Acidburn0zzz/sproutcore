var outcome=0;
var webpage = require('webpage')

function runTest(idx, cb) {
    if(idx==urls.length) {
        cb.apply();
    }
    var url=urls[idx].url;
    var page = webpage.create();
    page.viewportSize = { width: 1024, height: 768 };
    // Chrome/Linux
    //page.settings.userAgent = "Mozilla/5.0 (X11; Linux i686) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/28.0.1500.71 Safari/537.36";
    page.open('http://'+host+':'+port+'/'+url, function (status) {
        // Check for page load success
        if (status !== "success") {
            console.log("["+url+"] tests failed: unable to access page");
            urls[idx].status=-1; outcome=1;
            page.close();
            runTest(++idx,cb);
        } else {
            waitFor(function() {
                // Check in the page if a specific element is now visible
                return page.evaluate(function() {
                    return $("span.status").text().indexOf("Running – Completed") == -1 ;
                });
            }, function() {
                // checking outcome
                var resultText = page.evaluate(function() {
                    return $("span.status").text();
                });
                urls[idx].resultText=resultText;
                if(resultText.indexOf("failed")!=-1) {
                  console.log("["+url+" tests failed: "+resultText);
                  urls[idx].status=1; outcome=1;
                } else if(resultText.indexOf("error")!=-1) {
                  console.log("["+url+" tests failed: "+resultText);
                  urls[idx].status=2; outcome=1;
                } else {
                  console.log("["+url+" tests passed: "+resultText);
                  urls[idx].status=0;
                }
                page.close();
                runTest(++idx,cb);
            });        
        }
    });
}

runTest(0, function() {
    phantom.exit(outcome);
});

